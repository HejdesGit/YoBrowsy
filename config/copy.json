{
  "dist": {
    "files": [
      {
        "expand": true,
        "dot": true,
        "cwd": "<%= yeoman.app %>",
        "dest": "<%= yeoman.dist %>",
        "src": [
          "*.{ico,png,txt}",
          ".htaccess",
          "images/{,*/}*.webp",
          "styles/fonts/{,*/}*.*"
        ]
      }
    ]
  },
  "styles": {
    "expand": true,
    "dot": true,
    "cwd": "<%= yeoman.app %>/styles",
    "dest": ".tmp/styles/",
    "src": "{,*/}*.css"
  }
}